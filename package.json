{
  "name": "knowledge-repo-boilerplate",
  "description": "Boilerplate to author github project at Knowledge.io",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "doctoc": "doctoc --github --title \"## Contents\" README.md",
    "build": "babel src -d dist --source-maps --plugins transform-object-rest-spread",
    "postinstall": "npm run build",
    "lint": "./node_modules/.bin/eslint src --fix"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "doctoc": "^1.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "standard": "^11.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true
    }
  },
  "author": "Knowledge - knowledge.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knowledge/knowledge-repo-boilerplate/issues"
  },
  "homepage": "https://github.com/knowledge/knowledge-repo-boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowledge/knowledge-repo-boilerplate.git"
  }
}
